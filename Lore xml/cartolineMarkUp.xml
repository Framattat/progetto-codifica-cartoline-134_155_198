        <text type="cartolina" xml:id="cartolina174">
            <!-- TRASCRIZIONE -->
            <body>
                <div type="fronte" facs="#F134">
                    <figure>
                        <graphic url="img/7694-134F.jpeg"/>
                        <figDesc>
                            L'immagine rappresenta una donna che guarda arrivare un carro carico di fieno.
                        </figDesc>
                        <head>Fieno ricchezza</head>
                    </figure>
                </div>

                <div type="retro" facs="#R174">
                    <div type="codice">
                        <p>
                            <num type="cardinal">
                                <idno facs="#cod">174</idno>
                            </num>
                        </p>
                    </div>
                    <div type="destination">
                        <p>
                            <!-- Timbro -->
                            <stamp type="postmark" facs="#timbro">
                                <mentioned rend="circle"><unclear reason="faded" resp="#AM">MODENA FERROVIA</unclear>
                                </mentioned>
                                <date when="1918-06-09">-9<unclear reason="faded">.6.</unclear>18</date>
                            </stamp>
                            <!-- Francobollo -->
                            <stamp type="postage" facs="#timbro">Francobollo da 5 centesimi del Regno d'Italia</stamp>
                        </p>
                        <!-- Indirizzo -->
                        <p style="indirizzo">
                            <address>
                                <addrLine facs="#ind1"> 
                                    <lb n="1"/>
                                    <choice> 
                                        <abbr> <hi rend="decorated">G</hi>ent<am>.</am></abbr> 
                                        <expan> Gent <ex> ilissima </ex> </expan>
                                    </choice>
                                    <choice>
                                        <abbr> <hi rend="decorated">S</hi>ig<am>.</am>na </abbr> 
                                        <expan> Sig<ex>nori</ex>na </expan>
                                    </choice>
                                </addrLine>
                                <addrLine facs="#ind2"> 
                                    <lb n="2"/>
                                    <persName  ref="#OT"> 
                                        <name type="person"><hi rend="decorated">O</hi>liva</name>
                                        <space/>
                                        <surname><hi rend="decorated">T</hi>urtura </surname>
                                    </persName> 
                                </addrLine>
                                <addrLine facs="#ind3"> 
                                    <lb n="3"/>
                                    <hi rend="decorated">V</hi>ia 
                                    <hi rend="decorated">G</hi>irolamo 
                                    <hi rend="decorated">R</hi>ossi 
                                    <num type="cardinal">58</num> 
                                </addrLine>
                                <addrLine facs="#ind4"> 
                                    <placeName ref="#RA"> 
                                        <lb n="4" /><district type="comune"><hi rend="decorated">Ravenna</hi></district> 
                                    </placeName> 
                                </addrLine>
                            </address>
                            </p>
                    </div>

                    <div type="typography">
                        <p><stamp type="typography" facs="#17410T">
                            <hi rend="decorated">S</hi> 
                            <hi rend="decorated">P</hi> 
                            <hi rend="decorated">A</hi>
                        </stamp></p>
                        <p><stamp type="typography" facs="#stamp" xml:lang="fr">
                            Pastorale en<placeName>Provence</placeName><pc type="punto">.</pc>
                        </stamp></p>
                        <p><stamp type="typography" facs="#stamp1">
                            Pastorale in <placeName>Provence</placeName><pc type="punto">.</pc>
                        </stamp></p>
                        <p><stamp type="typography" facs="#stamp2">
                        	<num>645</num><pc type="punto">.</pc><persName><abbr>J</abbr><expan resp="#AM">Jean</expan><pc type="punto">.</pc><abbr>B</abbr><expan resp="#AM">Baptiste</expan><pc type="punto">.</pc>DUFFAUD</persName><pc type="punto">.</pc>
                        </stamp></p>
                        
                        <p><stamp type="typography" facs="#stamp3">
                            Salon de <placeName>Paris</placeName><pc type="punto">.</pc>
                        </stamp></p>
                       
                        <p><stamp type="typography" facs="#stamp4">
                            <gap reason="illegible" agent="faded"/>
                        </stamp></p>
                    </div>

                    <div type="message">
                        <opener>
                            <dateline n="1" facs="#luogo">
                                <placeName>S<pc type="punto">.</pc>Felice</placeName>
                                <distinct>li</distinct>
                                <date when="1918-04-08"><num>8</num><pc type="barra">/</pc><num>4</num><pc type="tratto">-</pc>1918</date><pc type="tratto">-</pc>
                            </dateline>
                        </opener>

                        <p>
                        
                            <lb n="5"/><span facs="#testo1"><unclear reason="illegible" resp="#AM">Intenti gentili</unclear></span>
                            <lb n="6"/><span facs="#testo2">verace e profondo affetto<pc type="punto">.</pc></span>
                        </p>
                        <closer>
                            <signed>
                                <lb n="7"/><span facs="#sal"><persName ref="#GC">tuo Giovannino</persName></span>
                            </signed>
                        </closer>
                    </div>
                </div>
            </body>
            <body>
                <div type="fronte" facs="#F198">
                    <figure>
                        <graphic url="7694-134R.jpeg"/>
                        <figDesc> Paesaggio in cui sono presenti alberi con foglie arancioni, pecore mentre pascolano e abitazioni in lontananza </figDesc>
                        <head>Melrose Abbay</head>
                    </figure>
                </div>
                <div type="retro" facs="#R198">
                	<p><idno type="n_cartolina" facs="#198_nc">198</idno></p>
                    <div type="message" facs="#198_msg">
                        <!-- messaggio del mittente, introduzione/saluto -->
                        <opener>
                            <placeName ref="MO">
                                <unclear reason="eccentric_ductus">
                                    <seg>Mo</seg><gap reason="eccentric_ductus"/><seg>na</seg>
                                </unclear>
                            </placeName>
                            <w>lì<pc type="dot">.</pc></w>
                            <date when="1918-02-13">
                                13
                                <pc type="slash">/</pc>
                                2
                                <pc type="dot">.</pc>
                                18
                            </date>
                        </opener>
                        <!-- corpo del messaggio -->
                        <p></p>
                        <!-- chiusura del messaggio -->
                        <closer>
                            <salute>
                                <unclear reason="eccentric_ductus">
                                    <seg><hi rend="decorated">A</hi>ffetuosi</seg><gap reason="eccentric_ductus"/>
                                </unclear>
                            </salute>
                            <signed>
                                <persName ref="#GC"><addName type="alias"><hi rend="decorated">G</hi>iovannino</addName></persName>
                            </signed>
                        </closer>
                    </div>
                    <div type="destination">
                        <!-- francobollo e timbro -->
                        <p>
                            <stamp type="postmark" facs="#198_timbro"> 
                                Moden <supplied reason="omitted-in-original"> a </supplied>
                                
                                <date> 
                                    <unclear reason="faded"> 13-21-8 <gap reason="illegible"/></unclear> 
                                </date>
                                Ferr <supplied reason="omitted-in-original"> ovia </supplied>
                                <note>
                                    Il timbro risulta decentrato rispetto la cartolina, mancano quindi delle lettere e numeri. Si presuppone che il timbro riporti: "modena e la dicitura "ferrovia".
                                </note>
                            </stamp>
                            <stamp type="postage" facs="#198_francob">
                                Francobollo da 5 centesimi del Regno d'Italia
                            </stamp>
                        </p>
                        <!-- Indirizzo -->
                        <p>
                            <address facs="#198_indiriz">
                                <addrLine> 
                                    <choice> 
                                        <abbr><hi rend="decorated">G</hi>ent</abbr> 
                                        <expan>Gentilissima</expan>
                                    </choice>
                                    <choice>
                                        <abbr><hi rend="decorated">S</hi>ig <damaged agent="ink" facs="#198_macchia"><gap reason="damaged"/></damaged>
                                            <add place="above">na</add>
                                        </abbr> 
                                        <expan>Signorina</expan>
                                    </choice>
                                </addrLine>
                                <addrLine> 
                                    <persName  ref="#OT">
                                        <hi rend="decorated">O</hi>liva
                                        <hi rend="decorated">T</hi>urtura
                                    </persName> 
                                </addrLine>
                                <addrLine> 
                                    <hi rend="decorated">V</hi>ia 
                                    <hi rend="decorated">G</hi>irolamo 
                                    <unclear reason="eccentric_ductus">
                                        <choice>
                                            <sic><hi rend="decorated">R</hi>osfi</sic>
                                            <corr>Rossi</corr>
                                        </choice>
                                    </unclear> 
                                    <num>58</num>
                                    <note>
                                        Dalla calligrafia non si comprende chiaramente la via di riferimento, si suppone sia Via Girolamo Rossi 58.
                                    </note> 
                                </addrLine>
                                <addrLine> 
                                    <placeName ref="#RA"><hi rend="decorated">R</hi>avenna</placeName> 
                                </addrLine>
                            </address>
                        </p>
                    </div>
                    <fw type="sig" place="left" facs="198_nota1" >Raphael Tuck &amp; Sons' "OILETTE" Postcard no 7015 ART PUBLISHER TO THEIR MAJESTIES THE KING &amp; QUEEN.</fw>
                    <fw type="front_desc" place="top-left" facs="198_nota2">
                        <note>Un box in cui troviamo la descrizione dell'immagine frontale della cartolina, descrive una zona della Scozia chiamata "Melrose Abbay"</note>
                    </fw>
                    <fw type="address_zone" place="center" facs="198_nota3">For addresses only</fw>
                    <fw type="header" place="top" facs="#198_header">Tuck's Post Card</fw>
                    <fw type="sig" place="top-left" facs="#198_logo">Logo della corona inglese</fw>
                    <fw type="footer" place="bottom-left" facs="#198_printed">Printed in england</fw>
                </div>
            </body>
        </text>
